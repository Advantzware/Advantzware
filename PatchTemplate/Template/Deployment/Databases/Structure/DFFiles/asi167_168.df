ADD TABLE "taskExecution"
  AREA "Data"
  DUMP-NAME "taskexecution"

ADD FIELD "taskID" OF "taskExecution" AS integer 
  DESCRIPTION "id, unique identifier, no user editing, sequence number or other automatic numbered item"
  FORMAT "->,>>>,>>9"
  INITIAL "0"
  LABEL "Task  ID"
  POSITION 2
  MAX-WIDTH 4
  ORDER 10
  MANDATORY

ADD FIELD "module" OF "taskExecution" AS character 
  DESCRIPTION "informational, but linked to module licensing"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Module"
  POSITION 3
  MAX-WIDTH 16
  ORDER 20

ADD FIELD "description" OF "taskExecution" AS character 
  DESCRIPTION "Description of monitor, what it does, how it does it, any other comments "
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Description"
  POSITION 4
  MAX-WIDTH 120
  ORDER 30

ADD FIELD "triggerEvennt" OF "taskExecution" AS character 
  DESCRIPTION "Trigger, what executes the program to execute ? manually run, scheduled, non-stop, etc. (Default options include: One Time, Recurring, On Startup)"
  FORMAT "x(24)"
  INITIAL ""
  LABEL "Trigger Event"
  POSITION 5
  MAX-WIDTH 48
  ORDER 40

ADD FIELD "startDT" OF "taskExecution" AS datetime 
  DESCRIPTION "The next date/time to start a task "
  FORMAT "99/99/9999 HH:MM:SS.SSS"
  INITIAL ?
  LABEL "Start Date/Time"
  POSITION 6
  MAX-WIDTH 8
  ORDER 50

ADD FIELD "endDT" OF "taskExecution" AS datetime 
  DESCRIPTION "The end time to execute the task ? Used to turn off the task"
  FORMAT "99/99/9999 HH:MM:SS.SSS"
  INITIAL ?
  LABEL "End D/T"
  POSITION 7
  MAX-WIDTH 8
  ORDER 60

ADD FIELD "triggerTime" OF "taskExecution" AS integer 
  DESCRIPTION "The amount of time to set for it to run next time (Set to zero if it should run continuously)"
  FORMAT "->,>>>,>>9"
  INITIAL "0"
  LABEL "Trigger Time"
  POSITION 8
  MAX-WIDTH 4
  ORDER 70

ADD FIELD "triggerUnit" OF "taskExecution" AS character 
  DESCRIPTION "This is the repeat which is used to determine the NEXT time the event should be run ? Minutes, Days
Example: Repeat every (Time) (Unit) ? Repeat every [15] [minutes], Repeat every [7] [Days]
"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Trigger Unit"
  POSITION 9
  MAX-WIDTH 16
  ORDER 80

ADD FIELD "taskType" OF "taskExecution" AS character 
  DESCRIPTION "(Chose one option) ? Program, Report, Utility, ASI 
Example: Initially this will just be run the program, but ultimately this could be an email, send text message, trigger something in the system, execute a backup, restore the test database, set up team viewer, etc.
"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Task Type"
  POSITION 10
  MAX-WIDTH 16
  ORDER 90

ADD FIELD "newSession" OF "taskExecution" AS logical 
  DESCRIPTION "This is used to indicate if the process should execute even if another session is required and utilizes a new user license"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "New Session"
  POSITION 11
  MAX-WIDTH 1
  ORDER 100

ADD FIELD "lastExecutionDT" OF "taskExecution" AS datetime 
  DESCRIPTION "This is the date of the last execution of this task that gets set once the task begins"
  FORMAT "99/99/9999 HH:MM:SS.SSS"
  INITIAL ?
  LABEL "Last Executed D/T"
  POSITION 12
  MAX-WIDTH 8
  ORDER 110

ADD FIELD "lastResult" OF "taskExecution" AS character 
  DESCRIPTION "reason code and description - Char(24) that is set once the task is either completed or there is a problem with the task to provide information to the user
Example: Success, Failed, Could not Login, No response, File locked, stack trace etc. and is just text so it is flexible for ASI to write any result desired and not editable by user.
"
  FORMAT "x(24)"
  INITIAL ""
  LABEL "Last Result"
  POSITION 13
  MAX-WIDTH 48
  ORDER 120

ADD FIELD "taskProgramName" OF "taskExecution" AS character 
  DESCRIPTION "Name of program to execute which can be a name or a reference to program master (To handle security, licensing, etc)"
  FORMAT "x(40)"
  INITIAL ""
  LABEL "Program Name"
  POSITION 14
  MAX-WIDTH 80
  ORDER 130

ADD FIELD "taskSettings" OF "taskExecution" AS character 
  DESCRIPTION "value to indicate the settings necessary to pass to the task (Like an execution string -n=40, -profiler, etc)"
  FORMAT "x(80)"
  INITIAL ""
  LABEL "Task Settings"
  POSITION 15
  MAX-WIDTH 160
  ORDER 140

ADD FIELD "startIn" OF "taskExecution" AS character 
  DESCRIPTION "Where the program should be located so it does not have to be in propath and can be any program reachable on the network"
  FORMAT "x(40)"
  INITIAL ""
  LABEL "Start In"
  POSITION 16
  MAX-WIDTH 80
  ORDER 150

ADD FIELD "dataFileName" OF "taskExecution" AS character 
  DESCRIPTION "The name or type of data file that will be utilized to execute the task (Not sure if needed) that includes wildcards (*.txt, Job*.dat, etc)
Example: ASI Data, ODBC data, FTP Site, XML, Excel, Text, Database, File system
"
  FORMAT "x(40)"
  INITIAL ""
  LABEL "Data File Name"
  POSITION 17
  MAX-WIDTH 80
  ORDER 160

ADD FIELD "dataFileType" OF "taskExecution" AS character 
  DESCRIPTION "The name or type of data file that will be utilized to execute the task (Not sure if needed) that includes wildcards (*.txt, Job*.dat, etc)
Example: ASI Data, ODBC data, FTP Site, XML, Excel, Text, Database, File system
"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Data File Type"
  POSITION 18
  MAX-WIDTH 16
  ORDER 170

ADD FIELD "dataLocation" OF "taskExecution" AS character 
  DESCRIPTION "The location of the data to use for this task ? Where to look for the data files (\\servername\inputfiles\, n:\custfiles\, etc.)"
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Data Location"
  POSITION 19
  MAX-WIDTH 120
  ORDER 180

ADD FIELD "results" OF "taskExecution" AS character 
  DESCRIPTION "Options for output of results ? Options include (None ? No result notifications, Audit Log ? Adds record to audit log, Log File ? Adds to some log file, Email ? Sends email, etc)"
  FORMAT "x(24)"
  INITIAL ""
  LABEL "Results"
  POSITION 20
  MAX-WIDTH 48
  ORDER 190

ADD FIELD "processedData" OF "taskExecution" AS character 
  DESCRIPTION "Location of raw data that has been processed (Any file that is processed, keep the original file)"
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Processed Data"
  POSITION 21
  MAX-WIDTH 120
  ORDER 200

ADD FIELD "rejectedData" OF "taskExecution" AS character 
  DESCRIPTION "Location of processed data set that did not process successfully (User intervention would be required if this has any data)"
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Rejected Data"
  POSITION 22
  MAX-WIDTH 120
  ORDER 210

ADD FIELD "retainNoResults" OF "taskExecution" AS integer 
  DESCRIPTION "Units of results to retain before purged (Either some time unit basis or number of results (From 0 (None) - 999 (All))"
  FORMAT ">>9"
  INITIAL "0"
  LABEL "Retain NumResults"
  POSITION 23
  MAX-WIDTH 4
  ORDER 220

ADD FIELD "retainUnits" OF "taskExecution" AS character 
  DESCRIPTION "The number of result sets or the number of days of results that are retained (Days OR Result sets)"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Retain Units"
  POSITION 24
  MAX-WIDTH 16
  ORDER 230

ADD FIELD "notificationEmail" OF "taskExecution" AS character 
  DESCRIPTION "Email for any notifications related to task (This would normally be a group or admin user)"
  FORMAT "x(40)"
  INITIAL ""
  LABEL "Notification Email"
  POSITION 25
  MAX-WIDTH 80
  ORDER 240

ADD FIELD "asiNotificationEmail" OF "taskExecution" AS character 
  DESCRIPTION "Email address for any notification sent to ASI"
  FORMAT "x(40)"
  INITIAL ""
  LABEL "ASI Notify Email"
  POSITION 26
  MAX-WIDTH 80
  ORDER 250

ADD FIELD "asiResults" OF "taskExecution" AS character 
  DESCRIPTION "Options for output of results ? Options include (None ? No result notifications, Audit Log ? Adds record to audit log, Log File ? Adds to log file, Email ? Sends email, etc)"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "ASI Results Options"
  POSITION 27
  MAX-WIDTH 16
  ORDER 260

ADD FIELD "isActive" OF "taskExecution" AS logical 
  DESCRIPTION "Active flag to indicate the task is active"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "Active"
  POSITION 28
  MAX-WIDTH 1
  ORDER 270

ADD FIELD "securityLevel" OF "taskExecution" AS integer 
  DESCRIPTION "Security level for editing of task (Default = 900)"
  FORMAT ">999"
  INITIAL "0"
  LABEL "Security Level"
  POSITION 29
  MAX-WIDTH 4
  ORDER 280

ADD FIELD "defaultSecurityLevel" OF "taskExecution" AS integer 
  DESCRIPTION "Default security level (Default = 900)"
  FORMAT ">999"
  INITIAL "0"
  LABEL "Def. Sec. Level"
  POSITION 30
  MAX-WIDTH 4
  ORDER 290

ADD FIELD "notifyUsers" OF "taskExecution" AS logical 
  DESCRIPTION "Indicate if this task should notify all uses logged into the system that this task will be excuted (Logical)
Example: 1020-260 ? The user is logged in and a message box displays a message ?System Backup has begun ? expect reduced performance until this task is completed?"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "Notify Users"
  POSITION 31
  MAX-WIDTH 1
  ORDER 300

ADD FIELD "forceLogout" OF "taskExecution" AS logical 
  DESCRIPTION "The system shall have the option of forcing users out of the system when this task begins if it requires restarting of the server, the database or other critical process (Objective)"
