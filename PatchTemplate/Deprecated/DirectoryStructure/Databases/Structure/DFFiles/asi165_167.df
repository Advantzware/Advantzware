ADD TABLE "costHeader"
  AREA "Data"
  LABEL "Cost Header"
  DESCRIPTION "Header tables for estimate and job cost calculations"
  DUMP-NAME "costHeader"

ADD FIELD "company" OF "costHeader" AS character 
  DESCRIPTION "Company code"
  FORMAT "x(3)"
  INITIAL ""
  LABEL "Company"
  POSITION 2
  MAX-WIDTH 6
  COLUMN-LABEL "Company"
  HELP "Enter the company code."
  ORDER 5
  MANDATORY

ADD FIELD "rec_key" OF "costHeader" AS character 
  DESCRIPTION "Unique identifier for header record"
  FORMAT "X(20)"
  INITIAL ""
  LABEL "Rec Key"
  POSITION 3
  MAX-WIDTH 40
  COLUMN-LABEL "Rec Key"
  HELP "Enter record key"
  ORDER 15

ADD FIELD "rec_keyParentForm" OF "costHeader" AS character 
  DESCRIPTION "Unique identifier for parent form"
  FORMAT "X(20)"
  INITIAL ""
  LABEL "Rec Key Form"
  POSITION 4
  MAX-WIDTH 40
  COLUMN-LABEL "Rec Key Form"
  HELP "Enter form record key"
  ORDER 25

ADD FIELD "rec_keyParentSet" OF "costHeader" AS character 
  DESCRIPTION "Unique identifier for parent set"
  FORMAT "X(20)"
  INITIAL ""
  LABEL "Rec Key Set"
  POSITION 5
  MAX-WIDTH 40
  COLUMN-LABEL "Rec Key Set"
  HELP "Enter set record key"
  ORDER 35

ADD FIELD "isItem" OF "costHeader" AS logical 
  DESCRIPTION "Does the header represent the costs for a finished good item"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "Is FG Item"
  POSITION 6
  MAX-WIDTH 1
  COLUMN-LABEL "Is FG Item"
  HELP "Select Yes if this is FG Item"
  ORDER 45

ADD FIELD "estimateNo" OF "costHeader" AS character 
  DESCRIPTION "Estimate number for header"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Estimate #"
  POSITION 7
  MAX-WIDTH 16
  COLUMN-LABEL "Estimate #"
  HELP "Enter estimate number."
  ORDER 55
  MANDATORY

ADD FIELD "industry" OF "costHeader" AS integer 
  DESCRIPTION "Holds an indicator for industry 1=Fold 2=Corr 3=Foam"
  FORMAT "9"
  INITIAL "0"
  LABEL "Industry"
  POSITION 8
  MAX-WIDTH 4
  COLUMN-LABEL "Industry"
  HELP "Enter whether header is used for 1=Fold 2=Corr 3=Foam"
  ORDER 65

ADD FIELD "estimateType" OF "costHeader" AS integer 
  DESCRIPTION "Estimate Type: 1 = Single Fold, 2 = Set Fold, 3/4 = Combo Fold
5 = Single Corr, 6 = Set Corr, 8 = Combo Corr"
  FORMAT "9"
  INITIAL "0"
  LABEL "Estimate Type"
  POSITION 9
  MAX-WIDTH 4
  COLUMN-LABEL "Estimate Type"
  HELP "Enter estimate type (1-Single 2-2pc. 3-Tandem 4-Combo)"
  ORDER 75

ADD FIELD "quantityMaster" OF "costHeader" AS integer 
  DESCRIPTION "Total Estimate Quantity for cost calculation (Single, Set, Total Combo & Tandem)"
  FORMAT ">>>>>>>9"
  INITIAL "0"
  LABEL "Quantity"
  POSITION 10
  MAX-WIDTH 4
  COLUMN-LABEL "Quantity"
  HELP "Enter master quantity for cost calculation"
  ORDER 85

ADD FIELD "headerDescription" OF "costHeader" AS character 
  DESCRIPTION "Description for the header cost calculation"
  FORMAT "x(30)"
  INITIAL ""
  LABEL "Header Description"
  POSITION 11
  MAX-WIDTH 60
  COLUMN-LABEL "Header Desc"
  HELP "Enter header description"
  ORDER 95

ADD FIELD "formNo" OF "costHeader" AS integer 
  DESCRIPTION "Form/Sheet Number"
  FORMAT ">9"
