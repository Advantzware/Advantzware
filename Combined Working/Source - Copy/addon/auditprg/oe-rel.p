  PUT STREAM s-audit-rpt UNFORMATTED "Order#: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel2.ord-no FORMAT ">>>>>9" " (ord-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel1.ord-no FORMAT ">>>>>9" " (ord-no)".
  PUT STREAM s-audit-rpt UNFORMATTED "Line: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel2.line FORMAT "99" " (line)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel1.line FORMAT "99" " (line)".
  PUT STREAM s-audit-rpt UNFORMATTED "Date Last Counted: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel2.rel-date FORMAT "99/99/99" " (rel-date)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel1.rel-date FORMAT "99/99/99" " (rel-date)".
  PUT STREAM s-audit-rpt UNFORMATTED "Ship To #: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel2.ship-no FORMAT ">>9" " (ship-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel1.ship-no FORMAT ">>9" " (ship-no)".
  PUT STREAM s-audit-rpt UNFORMATTED "Item No: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel2.i-no " (i-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rel1.i-no " (i-no)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
