  PUT STREAM s-audit-rpt UNFORMATTED "Invoice No.: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_ap-inv2.inv-no " (inv-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_ap-inv1.inv-no " (inv-no)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
