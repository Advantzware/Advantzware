  PUT STREAM s-audit-rpt UNFORMATTED "Updated Time: " AT 3 ttbl_oe-rell1.upd-time FORMAT "->,>>>,>>9" " (upd-time)".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_oe-rell1.upd-time NE ttbl_oe-rell2.upd-time THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "Updated Time: " AT 3 ttbl_oe-rell2.upd-time FORMAT "->,>>>,>>9".
  END.
END.
IF v_show_index THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "Index Fields - Label: Value (database field-name)" AT 5.
  PUT STREAM s-audit-rpt UNFORMATTED "Company: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rell2.company " (company)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rell1.company " (company)".
  PUT STREAM s-audit-rpt UNFORMATTED "Customer PO: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rell2.po-no " (po-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_oe-rell1.po-no " (po-no)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
