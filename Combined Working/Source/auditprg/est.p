  PUT STREAM s-audit-rpt UNFORMATTED "Updated Date: " AT 3 ttbl_est1.updated-date FORMAT "99/99/99" " (updated-date)".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_est1.updated-date NE ttbl_est2.updated-date THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "Updated Date: " AT 3 ttbl_est2.updated-date FORMAT "99/99/99".
  END.
END.
IF NOT v_field_changed OR ttbl_est1.updated-id NE ttbl_est2.updated-id OR action NE "UPDATE" THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "Updated by: " AT 3 ttbl_est1.updated-id " (updated-id)".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_est1.updated-id NE ttbl_est2.updated-id THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "Updated by: " AT 3 ttbl_est2.updated-id.
  END.
END.
IF v_show_index THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "Index Fields - Label: Value (database field-name)" AT 5.
  PUT STREAM s-audit-rpt UNFORMATTED "Company: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_est2.company " (company)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_est1.company " (company)".
  PUT STREAM s-audit-rpt UNFORMATTED "Warehouse: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_est2.loc " (loc)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_est1.loc " (loc)".
  PUT STREAM s-audit-rpt UNFORMATTED "Estimate #: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_est2.est-no " (est-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_est1.est-no " (est-no)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
