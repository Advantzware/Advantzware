  PUT STREAM s-audit-rpt UNFORMATTED "PO#: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rm-rctd2.po-no " (po-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rm-rctd1.po-no " (po-no)".
  PUT STREAM s-audit-rpt UNFORMATTED "Warehouse: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rm-rctd2.loc " (loc)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rm-rctd1.loc " (loc)".
  PUT STREAM s-audit-rpt UNFORMATTED "Primary Bin Loc.: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rm-rctd2.loc-bin " (loc-bin)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rm-rctd1.loc-bin " (loc-bin)".
  PUT STREAM s-audit-rpt UNFORMATTED "?: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rm-rctd2.tag " (tag)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rm-rctd1.tag " (tag)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
