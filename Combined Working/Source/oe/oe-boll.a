/* ---------------------------------------------------------- oe/oe-boll.a bg */
/* add module - order entry BOL     lines                                     */
/* -------------------------------------------------------------------------- */

find oe-bolh where recid(oe-bolh) = bolh_id no-lock.

   create oe-boll.
   assign
      oe-boll.company    = cocode
      oe-boll.loc        = locode
      oe-boll.bol-no     = oe-bolh.bol-no
      oe-boll.ord-no     = oe-bolh.ord-no
      oe-boll.po-no      = oe-bolh.po-no
      oe-boll.r-no       = oe-bolh.r-no
      nufile             = true
      oe-boll.b-no       = oe-bolh.b-no.

  if available oe-boll then
  assign fil_id   = recid(oe-boll)
         boll_id  = recid(oe-boll).

