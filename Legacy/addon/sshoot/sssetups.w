DO:
     RUN fg/fgpstall.w PERSISTENT (?,"SETUP").
    {Advantzware/WinKit/winkit-panel-triggerend.i} /* added by script _nonAdm1.p on 04.07.2017 @  2:06:17 pm */
END.

