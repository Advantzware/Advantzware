IF v_show_index THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "Index Fields - Label: Value (database field-name)" AT 5.
  PUT STREAM s-audit-rpt UNFORMATTED "Company: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_po-ord2.company " (company)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_po-ord1.company " (company)".
  PUT STREAM s-audit-rpt UNFORMATTED "PO Number: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_po-ord2.po-no FORMAT ">>>>>9" " (po-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_po-ord1.po-no FORMAT ">>>>>9" " (po-no)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
