  PUT STREAM s-audit-rpt UNFORMATTED "Sales Tax Group: " AT 3 ttbl_po-ord1.tax-gr " (tax-gr)".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_po-ord1.tax-gr NE ttbl_po-ord2.tax-gr THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "Sales Tax Group: " AT 3 ttbl_po-ord2.tax-gr.
  END.
END.
