&ANALYZE-SUSPEND _UIB-CODE-BLOCK _FUNCTION fGetURL W-Win 
FUNCTION fGetURL RETURNS CHARACTER
  ( ) :
    /*------------------------------------------------------------------------------
     Purpose: Returns NK1 value of the JobQueueURL setting
     Notes:
    ------------------------------------------------------------------------------*/
    DEFINE VARIABLE cURL     AS CHARACTER NO-UNDO.
    DEFINE VARIABLE lFound   AS LOGICAL   NO-UNDO.
    DEFINE VARIABLE cCompany AS CHARACTER NO-UNDO.
    DEFINE VARIABLE cUserID  AS CHARACTER NO-UNDO.
    
    RUN spGetSessionParam ("Company", OUTPUT cCompany).
    RUN spGetSessionParam ("UserID", OUTPUT cUserID).
    
    RUN sys/ref/nk1look.p (
        INPUT  cCompany,
        INPUT  "JobQueueURL",
        INPUT  "C",
        INPUT  NO,
        INPUT  NO,
        INPUT  "",
        INPUT  "",
        OUTPUT cURL, 
        OUTPUT lFound
        ).
    IF cUserID NE "" THEN 
        cURL = cURL + "?UserId=" + cUserID.
        
    RETURN cURL.

END FUNCTION.

