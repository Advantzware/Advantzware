PROCEDURE fixACCOUNT:
  DEFINE VARIABLE i AS INTEGER NO-UNDO.

  DISABLE TRIGGERS FOR LOAD OF account.
 
  IF NOT CAN-FIND(FIRST account WHERE account.rec_key EQ '') THEN RETURN.
  RUN msg ('Processing account ... ').
  FOR EACH account EXCLUSIVE-LOCK WHERE account.rec_key EQ '':
    ASSIGN
      account.rec_key = nextRecKey()
      i = i + 1.
    RUN createRecKey (account.rec_key,'account').
  END.
  RUN msg ('Fixed ' + STRING(i) + ' Records' + CHR(10)).
END PROCEDURE.
 
