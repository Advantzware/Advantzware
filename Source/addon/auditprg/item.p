    PUT STREAM s-audit-rpt UNFORMATTED "Substitute Vendor: " AT 3 ttbl_item2.vend2-no.
  END.
END.
IF NOT v_field_changed OR ttbl_item1.yield NE ttbl_item2.yield OR action NE "UPDATE" THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "Yield Factor: " AT 3 ttbl_item1.yield FORMAT ">>>,>>9" " (yield)".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_item1.yield NE ttbl_item2.yield THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "Yield Factor: " AT 3 ttbl_item2.yield FORMAT ">>>,>>9".
  END.
END.
IF v_show_index THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "Index Fields - Label: Value (database field-name)" AT 5.
  PUT STREAM s-audit-rpt UNFORMATTED "Company: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_item2.company " (company)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_item1.company " (company)".
  PUT STREAM s-audit-rpt UNFORMATTED "Industry: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_item2.industry " (industry)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_item1.industry " (industry)".
  PUT STREAM s-audit-rpt UNFORMATTED "Name: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_item2.i-name " (i-name)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_item1.i-name " (i-name)".
  PUT STREAM s-audit-rpt UNFORMATTED "Item No: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_item2.i-no " (i-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_item1.i-no " (i-no)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
