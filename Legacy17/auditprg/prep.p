  PUT STREAM s-audit-rpt UNFORMATTED "SIMON: " AT 3 ttbl_prep1.simon " (simon)".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_prep1.simon NE ttbl_prep2.simon THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "SIMON: " AT 3 ttbl_prep2.simon.
  END.
END.
