define variable j as integer no-undo.
do while true:
  file-info:file-name = "readprobe.flg".
  if file-info:full-pathname = ? then leave.
  case random( 1,   9 ):
    when   1 then for each Invoice       no-lock: end.
    when   2 then for each Customer      no-lock: end.
    when   3 then for each Item          no-lock: end.
    when   4 then for each Order         no-lock: end.
    when   5 then for each Order-Line    no-lock: end.
    when   6 then for each Salesrep      no-lock: end.
    when   7 then for each State         no-lock: end.
    when   8 then for each Local-Default no-lock: end.
    when   9 then for each Ref-Call      no-lock: end.
  end.
  /***
  j = j + 1.
  if j modulo 50 = 0 then pause 1 no-message.
   ***/
end.
