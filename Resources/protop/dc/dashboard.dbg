     1094   
     1095   function fixDateString returns character ( input dx as character ):
     1096   
     1097     define variable d as character no-undo.
     1098     define variable m as character no-undo.
     1099     define variable y as character no-undo.
     1100     define variable t as character no-undo.
     1101   
     1102     /* The date-time field in _checkpoint looks like: Mon Jan 14 03:33:08 2013 */
     1103   
     1104     assign
     1105       m = string( lookup( substring( dx, 5, 3 ), "Jan,Feb,Mar,Apr,May,Jun,Jul,Aug,Sep,Oct,Nov,Dec" ), "99" )
     1106       d = string( integer( trim( substring( dx, 9, 2 ))), "99" )
     1107       y = substring( dx, 21 )
     1108       t = substring( dx, 12, 8 )
     1109     .
     1110   
     1111     if       session:date-format = "ymd" then return substitute( "&1-&2-&3 &4", y, m, d, t ).
     1112      else if session:date-format = "mdy" then return substitute( "&1-&2-&3 &4", m, d, y, t ).
     1113      else if session:date-format = "dmy" then return substitute( "&1-&2-&3 &4", d, m, y, t ).
     1114      else
     1115       do:
     1116         message "Unsupported session:date-format" session:date-format view-as alert-box.
     1117         quit.
     1118       end.
     1119   
     1120   end.
     1121   
     1122   
     1123   &IF DEFINED( OE10 ) &THEN
     1124   define variable lastSample as datetime no-undo.
     1125   &ELSE
     1126   define variable lastSample as integer  no-undo.
     1127   &ENDIF
     1128   
     1129   procedure calcChkptLen:
     1130   
     1131   &IF DEFINED( OE10 ) &THEN
     1132     define variable cpStart   as datetime no-undo.
     1133     define variable cpFinish  as datetime no-undo.
     1134   &ELSE
     1135     define variable cpStart   as integer  no-undo.
     1136     define variable cpFinish  as integer  no-undo.
     1137   &ENDIF
     1138   
     1139     define variable cpLength  as integer  no-undo.
     1140     define variable minLength as integer  no-undo initial 86399.          /* makes it easy to recognize a bogus result i.e. 23:59:59      */
     1141   
     1142     define variable n as integer no-undo.
     1143   
     1144   &IF DEFINED( OE10 ) &THEN
     1145   
     1146     for each dictdb._Checkpoint no-lock:
     1147   
     1148       /* the first record is usually incomplete
     1149        */
     1150   
     1151       if _Checkpoint._Checkpoint-Len = ? then
     1152         do:
     1153           next.
     1154         end.
     1155   
     1156       /* capture anything that happened during the sample period
     1157        */
     1158   
     1159       if datetime( fixDateString( _Checkpoint._Checkpoint-Time )) > lastSample then
     1160         do:
     1161           assign
     1162             cpStart  = datetime( fixDateString( _Checkpoint._Checkpoint-Time ))
     1163             cpFinish = datetime( fixDateString( _Checkpoint._Checkpoint-Len )) when n = 0
     1164             n = n + 1
     1165           .
     1166           if cpStart <> ? and cpFinish <> ? then
     1167             minLength = min( minLength, abs( interval( cpFinish, cpStart, "seconds" ))).
     1168           next.
     1169         end.
     1170   
     1171       /* capture the start of any partial chkpt that was active at the start of the sample
     1172        */
     1173   
     1174       if datetime( fixDateString( _Checkpoint._Checkpoint-Time )) <= lastSample then
     1175         do:
     1176           assign
     1177             cpStart  = datetime( fixDateString( _Checkpoint._Checkpoint-Time ))
     1178             cpFinish = datetime( fixDateString( _Checkpoint._Checkpoint-Len )) when n = 0
     1179             n = n + 1
     1180           .
     1181           if cpStart <> ? and cpFinish <> ? then
     1182             minLength = min( minLength, abs( interval( cpFinish, cpStart, "seconds" ))).
     1183           leave.  /* only get one such record */
     1184         end.
     1185   
     1186     end.
     1187   
     1188     if n > 0 then
     1189       assign
     1190         cpLength = abs( interval( cpFinish, cpStart, "seconds" )) / n
     1191         tt_Dashboard.avgChkPtLen = cplength
     1192         tt_Dashboard.avgChkPtStr = string( integer( cpLength ), "hh:mm:ss" )
     1193       .
     1194   
     1195     if minLength < 86399 and minLength > 0 then
     1196       assign
     1197         tt_Dashboard.minChkPtLen = minLength
     1198         tt_Dashboard.minChkPtStr = string( integer( minLength ), "hh:mm:ss" )
     1199       .
     1200   
     1201   &ENDIF
     1202   
     1203     return.
     1204   
     1205   end.
