      827   define new global shared variable rawMode  as integer no-undo initial 5.
      828   define new global shared variable timeMode as integer no-undo initial 2.
      829   
      830   define new global shared variable x as integer no-undo initial 5.
      831   define new global shared variable z as decimal no-undo.
      832   
      833   define variable y as decimal no-undo.
      834   
      835   define variable initTick as integer no-undo.
      836   define variable prevTick as integer no-undo.
      837   
      838   
      839   procedure updTick:
      840   
      841   &IF DEFINED( OE10 ) &THEN
      842     if initTick = 0 then initTick = mtime.
      843     assign
      844       x = rawMode /* ( if rawMode then 4 else 5 ) */
      845       z = mtime - prevTick
      846       z = ( if z >= 0 then z else 86400000 - prevTick + mtime )   /* handle rolling over midnight */
      847       z = z / 1000
      848       y = mtime - initTick
      849       y = y / 1000
      850       prevTick = mtime
      851     .
      852   &ELSE
      853     if initTick = 0 then initTick = etime.
      854     assign
      855       x = rawMode /* ( if rawMode then 4 else 5 ) */
      856       z = etime - prevTick
      857       z = ( if z >= 0 then z else 5000 )   /* etime doesn't rollover at midnite -- but a mistaken etime(yes) somewhere would be bad... */
      858       z = z / 1000
      859       y = etime - initTick
      860       y = y / 1000
      861       prevTick = etime
      862     .
      863   &ENDIF
      864   
      865     /* if rawMode <> 5 then z = 1. */
      866   
      867     if timeMode = 1 then z = 1.
      868   
      869     return.
      870   
      871   end.
      872   
      873   /* end tick.i */
