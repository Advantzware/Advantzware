     1043           lastAICurr  = tt_Dashboard.ai_curr
     1044         .
     1045   
     1046         assign
     1047           tt_Dashboard.ai_full   = 0
     1048           tt_Dashboard.ai_empty  = 0
     1049           tt_Dashboard.ai_locked = 0
     1050         .
     1051   
     1052         if hasAIInfo = yes then
     1053           do:
     1054   
     1055             for each _areaStatus no-lock where _areaStatus-areaName begins "after image area":
     1056               assign
     1057                 tt_Dashboard.ai_full   = tt_Dashboard.ai_full   + ( if ( buffer _areaStatus:handle:buffer-field( "_areaStatus-State" ):buffer-value ) = "Full"   then 1 else 0 )
     1058                 tt_Dashboard.ai_empty  = tt_Dashboard.ai_empty  + ( if ( buffer _areaStatus:handle:buffer-field( "_areaStatus-State" ):buffer-value ) = "Empty"  then 1 else 0 )
     1059                 tt_Dashboard.ai_locked = tt_Dashboard.ai_locked + ( if ( buffer _areaStatus:handle:buffer-field( "_areaStatus-State" ):buffer-value ) matches "*Lock*" then 1 else 0 )
     1060               no-error.
     1061             end.
     1062   
     1063           end.
     1064   
     1065         if hasAIInfo = no and pt_useRFUTIL = yes then
     1066           do:
     1067   
     1068             if opsys = "unix" then
     1069               input stream inStrm through value( substitute(      "rfutil &1 -C aimage list -cpinternal &2 -cpstream &3", pdbname(1), session:cpinternal, session:cpstream )).
     1070              else
     1071               input stream inStrm through value( substitute( 'call "%DLC%~\bin~\rfutil" &1 -C aimage list -cpinternal &2 -cpstream &3 2>> &4~\&5.rfutil.err', pdbname(1), session:cpinternal, session:cpstream, pt_tmpdir, pt_shortName )).
     1072   
     1073             repeat:
     1074               stuff = "".
     1075               import stream inStrm stuff.
     1076               if stuff[1] = "Status:" then
     1077                 do:
     1078                   if       stuff[2] = "Full"   then tt_Dashboard.ai_full   = tt_Dashboard.ai_full   + 1.
     1079                    else if stuff[2] = "Empty"  then tt_Dashboard.ai_empty  = tt_Dashboard.ai_empty  + 1.
     1080                    else if stuff[2] matches "*Lock*" then tt_Dashboard.ai_locked = tt_Dashboard.ai_locked + 1.
     1081                 end.
     1082             end.
     1083   
     1084             input stream inStrm close.
     1085   
     1086           end.
     1087   
     1088       end.
     1089   
     1090     return.
     1091   
     1092   end.
