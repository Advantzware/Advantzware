     3499   
     3500     /* a couple of very gross measures of how the OS is behaving...
     3501      *
     3502      * these might help to shine some light and distinguish between db issues vs OS issues.
     3503      * especially when SANs and VMs are in use.
     3504      *
     3505      */
     3506   
     3507     x = 0.
     3508     n = num-entries( pt_ioFileName ).
     3509   
     3510     estart = etime.
     3511   
     3512     /* if there are pending commands at the UI level skip this
     3513      */
     3514   
     3515     if session:batch or lastkey < 0 then
     3516       do:
     3517   
     3518         do while x <= pt_ioresp and ( etime - estart ) < 100:     /* limit this to 100ms of execution time                        */
     3519   
     3520           assign
     3521             i = i + 1
     3522             x = x + 1
     3523           .
     3524   
     3525           file-info:filename = ( if n > 1 then entry( random( 1, n ), pt_ioFileName ) else pt_ioFileName ).
     3526   
     3527           if file-info:full-pathname <> ? and index( file-info:file-type, "F" ) > 0 and index( file-info:file-type, "R" ) > 0 and file-info:file-size >= 1 then
     3528             do:
     3529               input stream ioTest from value( file-info:full-pathname ).
     3530   &IF DEFINED( OE10 ) &THEN
     3531               seek stream ioTest to random( 0, file-info:file-size - 1 ).
     3532   &ELSE
     3533               seek stream ioTest to random( 0, max( file-info:file-size - 1, exp( 2, 31 ) - 1 )).
     3534   &ENDIF
     3535               readkey stream ioTest.
     3536               input stream ioTest close.
     3537             end.
     3538   
     3539         end.
     3540   
     3541         tt_Dashboard.ioResponse = ( etime - estart ) / i.         /* calculate avg seek time in ms                                */
     3542   
     3543       end.
     3544   
     3545     /* tt_Dashboard.ioResponse = i / (( etime - estart ) / 1000 ). */     /* calculate IO ops/sec                                 */
     3546   
     3547     tt_Dashboard.bogoMIPS = ?.
     3548   
     3549     if bogoLoop < 10 then bogoLoop = bogoLoop + 1.                /* skip calculating this during startup                         */
     3550   
     3551     if pt_bogomips > 0 /* and bogoLoop > 1 */ then                /* or maybe don't skip it after all ;)                          */
     3552       do:
     3553   
     3554         estart = etime.
     3555         do i = 1 to pt_bogomips:                                  /* 100,000 should take 20ms to 30ms on circa 2010 HW            */
     3556         end.
     3557   
     3558         tt_Dashboard.bogoMIPS = (( pt_bogomips / (( etime - estart ) / 1000 )) / 1000000 ).
     3559   
     3560         if tt_Dashboard.bogoMIPS > 0 and tt_Dashboard.bogoMIPS <> ? then
     3561           do:
     3562             maxBogo = max( maxBogo, tt_Dashboard.bogoMIPS ).
     3563             tt_Dashboard.bogoMipPct = ( tt_Dashboard.bogoMIPS / maxBogo ) * 100.
     3564           end.
