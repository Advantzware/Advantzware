     3718       fBackUp = ( currDT - string2uDateTime( _dbStatus-fbDate ))
     3719       iBackUp = ( currDT - string2uDateTime( _dbStatus-ibDate ))
     3720     .
     3721   
     3722     tt_Dashboard.dbBkUpFull = fBackUp.
     3723   
     3724     if _dbStatus-fbDate = ? and _dbStatus-fbDate = ? then
     3725       tt_dashboard.dbBkUpAge  = "Never!".
     3726      else
     3727       do:
     3728         bkupSecs = min( fBackUp, iBackUp ).
     3729         if fBackup = ? then bkupSecs = iBackUp.
     3730         if iBackup = ? then bkupSecs = fBackUp.
     3731         assign
     3732           bkupDays = integer( truncate( bkupSecs / 86400, 0 ))
     3733           bkupSecs = bkupSecs modulo 86400
     3734           tt_dashboard.dbBkUpAge  = ( if bkupDays > 0 then string( bkupDays ) + "d " else "" ) + string( bkupSecs, "hh:mm" ).
     3735         .
     3736       end.
     3737       
     3738     if tt_dashboard.dbBkUpAge = ? then tt_dashboard.dbBkUpAge = "Unknown".
     3739   
     3740     assign
     3741       upSecs = _Summary-upTime
     3742       upDays = integer( truncate( upSecs / 86400, 0 ))
     3743       upSecs = upSecs modulo 86400
     3744       tt_dashboard.dbUpTime  = ( if upDays > 0 then string( upDays ) + "d " else "" ) + string( upSecs, "hh:mm" )
     3745     .
