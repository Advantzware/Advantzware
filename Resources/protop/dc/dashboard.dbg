      195   /** Global Shared Temp Table Definitions
      196    **
      197    ** Yup, they're shared.  But this stuff makes no sense across session boundaries anyway.
      198    ** And a shared temp-table is logically the same as a db table so who really cares?
      199    **
      200    **/
      201   
      202   /* cache _File and _Index records so that we don't keep hitting the db to translate
      203    */
      204   
      205   define new global shared temp-table tt_tbl no-undo
      206     field xid      as integer                                             /* _File._File-Num              */
      207     field tstatid  as integer                                             /* _TableStat._TableStat-Id     */
      208     field areaNum  as integer                                             /* _Storage-Object._Area-Number */
      209     field tblPool  as character                                           /* get-bits( _object-attrib, 7, 1 ) = 1 */
      210     field tblname  as character                                           /* _File._File-Name             */
      211     index xid-idx is unique primary xid.
      212   
      213   define new global shared temp-table tt_idx no-undo
      214     field xid      as integer                                             /* _Index._Idx-Num              */
      215     field istatid  as integer                                             /* _IndexStat._IndexStat-Id     */
      216     field idxname  as character                                           /* _Index._Idx-Name             */
      217     field idxnote  as character
      218     field idxRoot  as int64                                           /* _Storage-Object._Object-Root */
      219     field tblnum   as integer                                             /* _File._File-Num              */
      220     field areaNum  as integer                                             /* _Storage-Object._Area-Number */
      221     field idxPool  as character                                           /* get-bits( _object-attrib, 7, 1 ) = 1 */
      222     field tblname  as character                                           /* _File._File-Name             */
      223     index xid-idx is unique primary xid.
      224   
      225   define new global shared temp-table tt_areaExtent no-undo
      226     field areaNum  as integer                                             /* _areaExtent._Area-Number     */
      227     field extNum   as integer                                             /* _areaExtent._Extent-Number   */
      228   
      229     field extSize  as decimal                                             /* _areaExtent._Extent-Size     */
      230     field extType  as integer                                             /* _areaExtent._Extent-Type     */
      231     field extPath  as character                                           /* _areaExtent._Extent-Path     */
      232   
      233     index ae-idx is unique primary areaNum extNum.
      234   
      235   define new global shared temp-table tt_area no-undo
      236     field xid      as integer    format ">>>9"
      237     field SANum    as integer    format ">>>9"        label "#"
      238     field areaPool as character  format "x(2)"        label "BX"
      239   /*field areaStatus-Id as {&BIGINT} format ">>>9"    label "Id" */
      240     field SAName   as character  format "x(30)"       label "Area Name"
      241     field allocGB  as decimal    format ">>>>>9.99"   label "Allocated"
      242     field varGB    as decimal    format ">>>>>9.99"   label "Variable"
      243     field totGB    as decimal    format ">>>>>>9.99"  label "Tot GB"
      244     field hiGB     as decimal    format ">>>>>9.99"   label "Hi Water" serialize-hidden
      245     field freeGB   as decimal    format ">>>>>9.99"   label "Free GB"
      246     field pctAlloc as decimal    format ">>>>>9%"     label "%Alloc"
      247     field pctLastX as decimal    format ">>>>9%"      label "%LastX"
      248   
      249     field blkszkb as integer     format ">>9"         label "BSZ"
      250     field rpb     as integer     format ">>9"         label "RPB"
      251     field clstrsz as integer     format ">>9"         label "CSZ"
      252   
      253     field numTbls as integer     format ">>>>9"       label "#Tbls"
      254     field numIdxs as integer     format ">>>>9"       label "#Idxs"
      255     field numLOBs as integer     format ">>>>9"       label "#LOBs"
      256   
      257     field numExts as integer     format ">>>>9"       label "#Exts"
      258     field hasVar  as logical     format "Yes/No"      label "Var?"
      259   
      260     field xnote   as character   format "x"           label "*"
      261   
      262     field areaMaxPct as decimal  format ">>9.999%"    label "Max%"
      263     field idx3264    as decimal  format ">>9.999%"    label "Bug%"
      264   
      265     index pctAlloc-idx is primary pctAlloc descending
      266     index pctLastX-idx pctLastX pctAlloc descending
      267     index allocGB-idx allocGB descending
      268     index totGB-idx totGB descending
      269     index xid-idx is unique xid
      270     index SANum-idx is unique SANum
      271     index SAName-idx is unique SAName
