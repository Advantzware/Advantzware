@echo off 

call #ENVPT3#\bin\protopenv.bat

set DB=dbname
set USER=sysprogress
set PASS=password

set PORT=9999
set HOST=localhost

rem	dump views
rem

sqlschema -u %USER% -a %PASS% -t SQL92.%% -o %DB%.sqlviews progress:T:%HOST%:%PORT%:%DB%

rem	dump grants
rem

sqlschema -u %USER% -a %PASS% -g %%.%% -o %DB%.sqlperm progress:T:%HOST%:%PORT%:%DB%
