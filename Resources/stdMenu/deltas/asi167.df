ADD TABLE "zipcode"
  AREA "Data"
  LABEL "Zip Codes"
  DESCRIPTION "Zip Codes"
  VALEXP "{methods/validate.del/zipcode.i}"
  VALMSG "Zip Code exists within other related Tables and can not be deleted."
  DUMP-NAME "zipcode"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "create.trg/zipcode.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "delete.trg/zipcode.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "write.trg/zipcode.p" CRC "?" 

ADD FIELD "zipcode" OF "zipcode" AS character 
  FORMAT "X(10)"
  INITIAL ""
  LABEL "Zip/Postal Code"
  POSITION 2
  MAX-WIDTH 20
  COLUMN-LABEL "Zip Code"
  HELP "Enter Zip Code"
  LENGTH 0
  ORDER 10

ADD FIELD "city" OF "zipcode" AS character 
  FORMAT "X(20)"
  INITIAL ""
  LABEL "City"
