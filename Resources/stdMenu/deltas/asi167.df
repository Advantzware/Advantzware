  POSITION 4
  MAX-WIDTH 4
  HELP "Enter Province/City Abbreviation"
  LENGTH 0
  ORDER 50

ADD FIELD "country" OF "zipcode" AS character 
  FORMAT "X(10)"
  INITIAL ""
  LABEL "Country"
  POSITION 5
  MAX-WIDTH 20
  HELP "Enter Country Name"
  LENGTH 0
  ORDER 60

