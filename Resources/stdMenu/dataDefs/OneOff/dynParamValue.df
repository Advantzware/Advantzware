ADD INDEX "pi-subjectID" ON "dynParamValue" 
  AREA "Index"
  UNIQUE
  PRIMARY
  DESCRIPTION "Primary Index by Subject ID"
  INDEX-FIELD "subjectID" ASCENDING 
  INDEX-FIELD "user-id" ASCENDING 
  INDEX-FIELD "prgmName" ASCENDING 
  INDEX-FIELD "paramValueID" ASCENDING 

ADD INDEX "si-paramValueID" ON "dynParamValue" 
  AREA "Index"
  DESCRIPTION "Secondary Index by Parameter Value ID"
  INDEX-FIELD "paramValueID" ASCENDING 

ADD INDEX "si-progName" ON "dynParamValue" 
  AREA "Index"
  DESCRIPTION "Secondary Index by Program Name"
