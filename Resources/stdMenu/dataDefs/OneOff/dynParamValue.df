ADD TABLE "dynParamValue"
  AREA "Data"
  LABEL "Dynamic Parameter Values"
  DESCRIPTION "Dynamic Parameter Values"
  DUMP-NAME "dynParamValue"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "create.trg/dynParamValue.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "delete.trg/dynParamValue.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "write.trg/dynParamValue.p" CRC "?" 

ADD FIELD "prgmName" OF "dynParamValue" AS character 
  FORMAT "x(10)"
  INITIAL ""
  LABEL "Program Name"
  POSITION 3
  MAX-WIDTH 20
  HELP "Enter Program Name"
  ORDER 30

ADD FIELD "user-id" OF "dynParamValue" AS character 
  FORMAT "x(10)"
  INITIAL ""
  LABEL "User ID"
  POSITION 4
  MAX-WIDTH 20
  HELP "Enter User ID"
  ORDER 20

ADD FIELD "paramValueID" OF "dynParamValue" AS integer 
  FORMAT "->,>>>,>>9"
  INITIAL "0"
  LABEL "Parameter Value ID"
  POSITION 5
  MAX-WIDTH 4
  COLUMN-LABEL "Value ID"
  HELP "Enter Parameter Value ID"
  ORDER 40

ADD FIELD "rec_key" OF "dynParamValue" AS character 
