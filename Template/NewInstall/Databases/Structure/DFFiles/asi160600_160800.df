ADD TABLE "addrPhone"
  AREA "Data"
  LABEL "Addrss/Phone Link"
  DUMP-NAME "addrphone"

ADD FIELD "rec_key" OF "addrPhone" AS character 
  FORMAT "X(21)"
  INITIAL ""
  LABEL "Record Key"
  POSITION 2
  MAX-WIDTH 40
  HELP "Enter Record Key Value"
  LENGTH 0
  ORDER 40

ADD FIELD "linkTable" OF "addrPhone" AS character 
  FORMAT "x(12)"
  INITIAL ""
  LABEL "Table"
  POSITION 3
  MAX-WIDTH 24
  ORDER 50

ADD FIELD "linkRecKey" OF "addrPhone" AS character 
  FORMAT "x(20)"
  INITIAL ""
  LABEL "Link RecKey"
  POSITION 4
  MAX-WIDTH 40
  ORDER 60

ADD FIELD "phoneType" OF "addrPhone" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Phone Type"
  POSITION 5
  MAX-WIDTH 16
  ORDER 70

ADD FIELD "dialPrefix" OF "addrPhone" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Prefix"
  POSITION 6
  MAX-WIDTH 16
  ORDER 80

ADD FIELD "dialCountry" OF "addrPhone" AS character 
  FORMAT "x(4)"
  INITIAL ""
  LABEL "Dial Country"
  POSITION 7
  MAX-WIDTH 8
  ORDER 90

ADD FIELD "dialArea" OF "addrPhone" AS character 
  FORMAT "x(4)"
  INITIAL ""
  LABEL "Area"
  POSITION 8
  MAX-WIDTH 8
  ORDER 100

ADD FIELD "dialNumber" OF "addrPhone" AS character 
  FORMAT "x(10)"
  INITIAL ""
  LABEL "Number"
  POSITION 9
  MAX-WIDTH 20
  ORDER 110

ADD FIELD "dialExt" OF "addrPhone" AS character 
  FORMAT "x(6)"
  INITIAL ""
  LABEL "Extension"
  POSITION 10
  MAX-WIDTH 12
  ORDER 120

ADD FIELD "dialSuffix" OF "addrPhone" AS character 
  FORMAT "x(4)"
  INITIAL ""
  LABEL "Suffix"
  POSITION 11
  MAX-WIDTH 8
  ORDER 130

ADD FIELD "dialString" OF "addrPhone" AS character 
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Dial String"
  POSITION 12
  MAX-WIDTH 120
  ORDER 140

ADD INDEX "iLink" ON "addrPhone" 
  AREA "Data"
  UNIQUE
  PRIMARY
  INDEX-FIELD "linkTable" ASCENDING 
  INDEX-FIELD "linkRecKey" ASCENDING 

ADD INDEX "iNumber" ON "addrPhone" 
  AREA "Data"
  UNIQUE
  INDEX-FIELD "dialCountry" ASCENDING 
  INDEX-FIELD "dialArea" ASCENDING 
  INDEX-FIELD "dialNumber" ASCENDING 
  INDEX-FIELD "dialExt" ASCENDING 
  INDEX-FIELD "linkTable" ASCENDING 
  INDEX-FIELD "rec_key" ASCENDING 

ADD INDEX "iRecKey" ON "addrPhone" 
  AREA "Data"
  UNIQUE
  INDEX-FIELD "rec_key" ASCENDING 

ADD TABLE "costHeader"
  AREA "Data"
  LABEL "Cost Header"
  DESCRIPTION "Header tables for estimate and job cost calculations"
  DUMP-NAME "costHeader"
  TABLE-TRIGGER "CREATE" OVERRIDE PROCEDURE "create.trg/costHeader.p" CRC "?" 
  TABLE-TRIGGER "DELETE" OVERRIDE PROCEDURE "delete.trg/costHeader.p" CRC "?" 
  TABLE-TRIGGER "WRITE" OVERRIDE PROCEDURE "write.trg/costHeader.p" CRC "?" 

ADD FIELD "company" OF "costHeader" AS character 
  DESCRIPTION "Company code"
  FORMAT "x(3)"
  INITIAL ""
  LABEL "Company"
  POSITION 2
  MAX-WIDTH 6
  COLUMN-LABEL "Company"
  HELP "Enter the company code."
  ORDER 5
  MANDATORY

ADD FIELD "rec_key" OF "costHeader" AS character 
  DESCRIPTION "Unique identifier for header record"
  FORMAT "X(21)"
  INITIAL ""
  LABEL "Rec Key"
  POSITION 3
  MAX-WIDTH 40
  COLUMN-LABEL "Rec Key"
  HELP "Enter record key"
  ORDER 15

ADD FIELD "rec_keyParentForm" OF "costHeader" AS character 
  DESCRIPTION "Unique identifier for parent form"
  FORMAT "X(20)"
  INITIAL ""
  LABEL "Rec Key Form"
  POSITION 4
  MAX-WIDTH 40
  COLUMN-LABEL "Rec Key Form"
  HELP "Enter form record key"
  ORDER 25

ADD FIELD "rec_keyParentSet" OF "costHeader" AS character 
  DESCRIPTION "Unique identifier for parent set"
  FORMAT "X(20)"
  INITIAL ""
  LABEL "Rec Key Set"
  POSITION 5
  MAX-WIDTH 40
  COLUMN-LABEL "Rec Key Set"
  HELP "Enter set record key"
  ORDER 35

ADD FIELD "isItem" OF "costHeader" AS logical 
  DESCRIPTION "Does the header represent the costs for a finished good item"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "Is FG Item"
  POSITION 6
  MAX-WIDTH 1
  COLUMN-LABEL "Is FG Item"
  HELP "Select Yes if this is FG Item"
  ORDER 45

ADD FIELD "estimateNo" OF "costHeader" AS character 
  DESCRIPTION "Estimate number for header"
  FORMAT "x(8)"
