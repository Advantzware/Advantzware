  DESCRIPTION "unique identifier"
  FORMAT "x(20)"
  INITIAL ""
  LABEL "Message ID"
  POSITION 2
  MAX-WIDTH 40
  ORDER 10

ADD FIELD "module" OF "zMessage" AS character 
  DESCRIPTION "System module for organization"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Module"
  POSITION 3
  MAX-WIDTH 16
  ORDER 20

ADD FIELD "msgNum" OF "zMessage" AS integer 
  DESCRIPTION "secondary key (after module) to locate record"
  FORMAT "9999"
  INITIAL "0"
  LABEL "Message Number"
  POSITION 4
  MAX-WIDTH 4
  ORDER 30

ADD FIELD "hotKey" OF "zMessage" AS character 
  DESCRIPTION "Where first used (May be used multiple places)"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Hot Key"
  POSITION 5
  MAX-WIDTH 16
  ORDER 40

ADD FIELD "otherHotKeys" OF "zMessage" AS character 
  DESCRIPTION "Is used in other programs (Long char field) like zoho hot key field"
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Other Hot Keys"
  POSITION 6
  MAX-WIDTH 120
  ORDER 50

ADD FIELD "msgName" OF "zMessage" AS character 
  FORMAT "x(40)"
  INITIAL ""
  LABEL "Name"
  POSITION 7
  MAX-WIDTH 80
  ORDER 60

ADD FIELD "defaultTitle" OF "zMessage" AS character 
  DESCRIPTION "Prints as title bar for message by default"
  FORMAT "x(40)"
  INITIAL ""
  LABEL "Default Title"
  POSITION 8
  MAX-WIDTH 80
  ORDER 70

ADD FIELD "currentTitle" OF "zMessage" AS character 
  DESCRIPTION "Customer changed title bar ? If not blank, use this instead of default"
  FORMAT "x(40)"
  INITIAL ""
  LABEL "Current Title"
  POSITION 9
  MAX-WIDTH 80
  ORDER 80

ADD FIELD "defaultMsg" OF "zMessage" AS character 
  DESCRIPTION "Prints as the default message by default"
  FORMAT "x(120)"
  INITIAL ""
  LABEL "Default Message"
  POSITION 10
  MAX-WIDTH 240
  ORDER 90

ADD FIELD "currMessage" OF "zMessage" AS character 
  DESCRIPTION "Customer changed message ? if not blank, use this instead of default"
  FORMAT "x(120)"
  INITIAL ""
  LABEL "Current Message"
  POSITION 11
  MAX-WIDTH 240
  ORDER 100

ADD FIELD "options" OF "zMessage" AS character 
  DESCRIPTION "List of buttons that can be displayed (Help is automatic if in debug mode)"
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Options"
  POSITION 12
  MAX-WIDTH 120
  ORDER 110

ADD FIELD "displayOptions" OF "zMessage" AS logical 
  DESCRIPTION "Logical ? Yes, No, Ask.  If Yes, always do the event (Can only be used when there is a single option to proceed), No ? Do not display and Never accept proceed option, ask ? prompt user as all messages do now, Default = Ask"
  FORMAT "yes/no"
  INITIAL ?
  LABEL "Display?"
  POSITION 13
  MAX-WIDTH 1
  ORDER 120

ADD FIELD "defSecLevel" OF "zMessage" AS integer 
  DESCRIPTION "Default security level for user to change message"
  FORMAT ">999"
  INITIAL "0"
  LABEL "Def. Sec. Level"
  POSITION 14
  MAX-WIDTH 4
  ORDER 130

ADD FIELD "currSecLevel" OF "zMessage" AS integer 
  DESCRIPTION "Actual security level the user sets and must be higher than our default"
  FORMAT ">999"
  INITIAL "0"
  LABEL "Curr. Sec. Level"
  POSITION 15
  MAX-WIDTH 4
  ORDER 140

ADD FIELD "userSuppress" OF "zMessage" AS logical 
  DESCRIPTION "logical (Default = false)"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "User Suppress"
  POSITION 16
  MAX-WIDTH 1
  ORDER 150

ADD FIELD "logMessage" OF "zMessage" AS logical 
  DESCRIPTION "Logical, If audit log entry should be created (Default = false)"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "Log Msg"
  POSITION 17
  MAX-WIDTH 1
  ORDER 160

ADD FIELD "msgType" OF "zMessage" AS character 
  DESCRIPTION "Progress message statements have ""types"" such as ""error"" ""info"" question, etc. I'd like to include the message ""type"" in the database"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Msg Type"
  POSITION 18
  MAX-WIDTH 16
  ORDER 170

ADD FIELD "rtnValue" OF "zMessage" AS character 
  DESCRIPTION "messages, particularly ""question"" types, are expected to return values to the calling program, even if simple responses like ""Do you want to continue?"". This should be included in the message file."
  FORMAT "x(24)"
  INITIAL ""
  LABEL "Return Value"
  POSITION 19
  MAX-WIDTH 48
  ORDER 180

ADD FIELD "contextParms" OF "zMessage" AS character 
  DESCRIPTION "When running a message, the programmer would often want to include context information, such as ""Customer CU123 already exists. Try again."". The ability to have formatted messages with programmer submitted variables would be beneficial, and possible if the database allowed it"
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Context Parameters"
  POSITION 20
  MAX-WIDTH 120
  ORDER 190

