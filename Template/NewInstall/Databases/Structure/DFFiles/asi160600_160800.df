  LABEL "Estimate #"
  POSITION 7
  MAX-WIDTH 16
  COLUMN-LABEL "Estimate #"
  HELP "Enter estimate number."
  ORDER 55
  MANDATORY

ADD FIELD "industry" OF "costHeader" AS integer 
  DESCRIPTION "Holds an indicator for industry 1=Fold 2=Corr 3=Foam"
  FORMAT "9"
  INITIAL "0"
  LABEL "Industry"
  POSITION 8
  MAX-WIDTH 4
  COLUMN-LABEL "Industry"
  HELP "Enter whether header is used for 1=Fold 2=Corr 3=Foam"
  ORDER 65

ADD FIELD "estimateType" OF "costHeader" AS integer 
  DESCRIPTION "Estimate Type: 1 = Single Fold, 2 = Set Fold, 3/4 = Combo Fold
5 = Single Corr, 6 = Set Corr, 8 = Combo Corr"
  FORMAT "9"
  INITIAL "0"
  LABEL "Estimate Type"
