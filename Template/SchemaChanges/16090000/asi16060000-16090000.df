  AREA "Data"
  DUMP-NAME "zmessage"

ADD FIELD "msgID" OF "zMessage" AS character 
  DESCRIPTION "unique identifier"
  FORMAT "x(20)"
  INITIAL ""
  LABEL "Message ID"
  POSITION 2
  MAX-WIDTH 40
  ORDER 10

