  MAX-WIDTH 16
  ORDER 70

ADD FIELD "dialPrefix" OF "addrPhone" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Prefix"
  POSITION 6
  MAX-WIDTH 16
  ORDER 80

ADD FIELD "dialCountry" OF "addrPhone" AS character 
  FORMAT "x(4)"
  INITIAL ""
  LABEL "Dial Country"
  POSITION 7
  MAX-WIDTH 8
  ORDER 90

ADD FIELD "dialArea" OF "addrPhone" AS character 
  FORMAT "x(4)"
  INITIAL ""
  LABEL "Area"
  POSITION 8
  MAX-WIDTH 8
  ORDER 100

ADD FIELD "dialNumber" OF "addrPhone" AS character 
  FORMAT "x(10)"
  INITIAL ""
  LABEL "Number"
  POSITION 9
  MAX-WIDTH 20
  ORDER 110

ADD FIELD "dialExt" OF "addrPhone" AS character 
  FORMAT "x(6)"
  INITIAL ""
  LABEL "Extension"
  POSITION 10
  MAX-WIDTH 12
  ORDER 120

ADD FIELD "dialSuffix" OF "addrPhone" AS character 
  FORMAT "x(4)"
  INITIAL ""
  LABEL "Suffix"
  POSITION 11
  MAX-WIDTH 8
  ORDER 130

ADD FIELD "dialString" OF "addrPhone" AS character 
  FORMAT "x(60)"
  INITIAL ""
  LABEL "Dial String"
  POSITION 12
  MAX-WIDTH 120
  ORDER 140

ADD INDEX "iLink" ON "addrPhone" 
  AREA "Data"
  UNIQUE
  PRIMARY
  INDEX-FIELD "linkTable" ASCENDING 
  INDEX-FIELD "linkRecKey" ASCENDING 

ADD INDEX "iNumber" ON "addrPhone" 
  AREA "Data"
  UNIQUE
  INDEX-FIELD "dialCountry" ASCENDING 
  INDEX-FIELD "dialArea" ASCENDING 
  INDEX-FIELD "dialNumber" ASCENDING 
  INDEX-FIELD "dialExt" ASCENDING 
  INDEX-FIELD "linkTable" ASCENDING 
  INDEX-FIELD "rec_key" ASCENDING 

ADD INDEX "iRecKey" ON "addrPhone" 
  AREA "Data"
  UNIQUE
  INDEX-FIELD "rec_key" ASCENDING 

