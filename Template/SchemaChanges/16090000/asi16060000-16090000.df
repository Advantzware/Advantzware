  POSITION 4
  MAX-WIDTH 4
  ORDER 30

ADD FIELD "hotKey" OF "zMessage" AS character 
  DESCRIPTION "Where first used (May be used multiple places)"
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Hot Key"
  POSITION 5
  MAX-WIDTH 16
  ORDER 40

ADD FIELD "otherHotKeys" OF "zMessage" AS character 
  DESCRIPTION "Is used in other programs (Long char field) like zoho hot key field"
