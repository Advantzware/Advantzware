  MAX-WIDTH 24
  ORDER 50

ADD FIELD "linkRecKey" OF "addrPhone" AS character 
  FORMAT "x(20)"
  INITIAL ""
  LABEL "Link RecKey"
  POSITION 4
  MAX-WIDTH 40
  ORDER 60

ADD FIELD "phoneType" OF "addrPhone" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "Phone Type"
