ADD FIELD "paramID" OF "dynPrgrmsPage" AS integer 
  FORMAT ">,>>>,>>9"
  INITIAL "0"
  LABEL "Parameter ID"
  POSITION 6
  MAX-WIDTH 4
  HELP "Enter Parameter ID"
  ORDER 50

ADD FIELD "rec_key" OF "dynPrgrmsPage" AS character 
  FORMAT "x(21)"
  INITIAL ""
