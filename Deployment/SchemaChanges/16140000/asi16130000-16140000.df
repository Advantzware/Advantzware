ADD TABLE "dynPrgrmsPage"
  AREA "Data"
  DUMP-NAME "dynprgrmspage"

ADD FIELD "prgmName" OF "dynPrgrmsPage" AS character 
  FORMAT "X(10)"
  INITIAL ""
  LABEL "Program"
  POSITION 2
  MAX-WIDTH 20
  HELP "Enter Program Name"
  ORDER 10
  MANDATORY

ADD FIELD "subjectID" OF "dynPrgrmsPage" AS integer 
  FORMAT ">,>>>,>>9"
  INITIAL "0"
  LABEL "Subject ID"
  POSITION 3
  MAX-WIDTH 4
  HELP "Enter Subject ID"
  ORDER 30

