  DESCRIPTION "Find all estCostMiscs for Estimate"
  INDEX-FIELD "company" ASCENDING 
  INDEX-FIELD "estimateNo" ASCENDING 
  INDEX-FIELD "formNo" ASCENDING 
  INDEX-FIELD "blankNo" ASCENDING 

ADD INDEX "rec_key" ON "estCostMisc" 
  AREA "Data"
  UNIQUE
  DESCRIPTION "Find a estCostMisc by rec_key"
  INDEX-FIELD "rec_key" ASCENDING 

ADD TABLE "estCostOperation"
  AREA "Data"
  LABEL "Estimate Cost Operation"
  DESCRIPTION "Calculated cost detail for each operation on the estimate. The estCostOperation is associated to a form (Printer) or a form/blank (gluer)."
  DUMP-NAME "estcostoperation"
  TABLE-TRIGGER "CREATE" NO-OVERRIDE PROCEDURE "create.trg\estCostOperation.p" CRC "?" 
  TABLE-TRIGGER "DELETE" NO-OVERRIDE PROCEDURE "delete.trg\estCostOperation.p" CRC "?" 
  TABLE-TRIGGER "WRITE" NO-OVERRIDE PROCEDURE "write.trg\estCostOperation.p" CRC "?" 

