  PUT STREAM s-audit-rpt UNFORMATTED "Stop Time: " AT 3 ttbl_mch-act1.stopp FORMAT "->,>>>,>>9" " (stopp)".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_mch-act1.stopp NE ttbl_mch-act2.stopp THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "Stop Time: " AT 3 ttbl_mch-act2.stopp FORMAT "->,>>>,>>9".
  END.
END.
DO i = 1 TO 50:
IF NOT v_field_changed OR ttbl_mch-act1.total-rate[i] NE ttbl_mch-act2.total-rate[i] OR action NE "UPDATE" THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "?[" AT 3 i "]: " ttbl_mch-act1.total-rate[i] FORMAT ">>>>9.99<<<" " (total-rate[" i "])".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_mch-act1.total-rate[i] NE ttbl_mch-act2.total-rate[i] THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "?[" AT 3 i "]: " ttbl_mch-act2.total-rate[i] FORMAT ">>>>9.99<<<".
  END.
END.
END.
