  PUT STREAM s-audit-rpt UNFORMATTED ttbl_quote2.e-num FORMAT ">>>>>9" " (e-num)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_quote1.e-num FORMAT ">>>>>9" " (e-num)".
  PUT STREAM s-audit-rpt UNFORMATTED "Quote #: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_quote2.q-no FORMAT ">>>>9" " (q-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_quote1.q-no FORMAT ">>>>9" " (q-no)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
