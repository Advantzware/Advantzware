  PUT STREAM s-audit-rpt UNFORMATTED "?: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_EDIVTran2.Partner " (Partner)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_EDIVTran1.Partner " (Partner)".
  PUT STREAM s-audit-rpt UNFORMATTED "?: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_EDIVTran2.Seq FORMAT ">>>>>>9" " (Seq)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_EDIVTran1.Seq FORMAT ">>>>>>9" " (Seq)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
