    PUT STREAM s-audit-rpt UNFORMATTED "Width: " AT 3 ttbl_rfqitem2.wid FORMAT ">9.99999".
  END.
END.
IF NOT v_field_changed OR ttbl_rfqitem1.xgrain NE ttbl_rfqitem2.xgrain OR action NE "UPDATE" THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "Crossgrain: " AT 3 ttbl_rfqitem1.xgrain " (xgrain)".
  IF action = "UPDATE" THEN
  DO:
    IF ttbl_rfqitem1.xgrain NE ttbl_rfqitem2.xgrain THEN
    PUT STREAM s-audit-rpt UNFORMATTED "*" AT 1.
    PUT STREAM s-audit-rpt UNFORMATTED "Crossgrain: " AT 3 ttbl_rfqitem2.xgrain.
  END.
END.
IF v_show_index THEN
DO:
  PUT STREAM s-audit-rpt UNFORMATTED "Index Fields - Label: Value (database field-name)" AT 5.
  PUT STREAM s-audit-rpt UNFORMATTED "Company: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rfqitem2.company " (company)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rfqitem1.company " (company)".
  PUT STREAM s-audit-rpt UNFORMATTED "Warehouse: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rfqitem2.loc " (loc)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rfqitem1.loc " (loc)".
  PUT STREAM s-audit-rpt UNFORMATTED "RFQ#: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rfqitem2.rfq-no FORMAT "->,>>>,>>9" " (rfq-no)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rfqitem1.rfq-no FORMAT "->,>>>,>>9" " (rfq-no)".
  PUT STREAM s-audit-rpt UNFORMATTED "Seq#: " AT 7.
  IF action = "UPDATE" THEN
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rfqitem2.seq FORMAT "->,>>>,>>9" " (seq)".
  ELSE
  PUT STREAM s-audit-rpt UNFORMATTED ttbl_rfqitem1.seq FORMAT "->,>>>,>>9" " (seq)".
END.
PUT STREAM s-audit-rpt UNFORMATTED SKIP(1).
